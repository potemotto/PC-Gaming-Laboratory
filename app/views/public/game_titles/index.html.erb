<div class="row">
  <div class="col-md-8 offset-md-2">
<h1>タイトル一覧</h1>
    <table class = "table table-hover">
      <thead>
        <tr>
          <th>タイトル名</th>
          <th>ゲームジャンル</th>
          <th colspan="1"></th>
        </tr>
      </thead>
      <tbody>
        <% @game_titles.each do |game_title| %>
        <tr>
          <td><%= game_title.name %></td>
          <td><%= game_title.genre.name %></td>
          <td><%= link_to "比較", game_title_path(game_title.id) ,class: "btn btn-primary" %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
 </div>
</div>