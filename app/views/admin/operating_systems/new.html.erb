<h1>Admin/operating_systems#new</h1>
<h1>OS情報登録ページ</h1>
<table class = "table">
  <thead>
    <h2>登録OS一覧</h2><br>
  </thead>
  <tbody>
    <%= form_with model: @systems, url: admin_systems_path do |f| %>
      <tr><td>
      OS名称
      </td><td>
      <%= f.text_field :name %>
      </td>
      
      <tr><td>
      ビット数
      </td><td>
      <%= f.select :bit_type, ["64bit": 0,"32bit": 1] %>
      </td>
      
      <tr><td>
      上限メモリ
      </td><td>
      <%= f.text_field :upper_memory_limit %>
      </td>
      
      <tr><td>
      DirectX
      </td><td>
      <%= f.text_field :DirectX %>
      </td>
      
      <tr><td>
      リリース年
      </td><td>
      <%= f.text_field :year_of_release %>
      </td>
      
      </tbody>
    </table>
    <%= f.submit "新規登録" %>
    <% end %>
    <br>