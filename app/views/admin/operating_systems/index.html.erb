<h1>Admin/operating_systems#index</h1>
<h1>OS情報一覧ページ</h1>
<%= link_to "OS登録ページ", new_admin_system_path %>

    <table class = "table table-hover">
      <thead>
        <tr>
          <th>OS名称</th>
          <th>ビット数</th>
          <th>上限メモリ</th>
          <th>DirectX</th>
          <th>リリース年</th>
          <th colspan="1"></th>
        </tr>
      </thead>
      <tbody>
        <% @systems.each do |system| %>
        <tr>
          <td><%= system.name %></td>
          <td><%= system.bit_type %></td>
          <td><%= system.upper_memory_limit %></td>
          <td><%= system.DirectX %></td>
          <td><%= system.year_of_release %></td>
          <td><%= link_to "編集", edit_admin_system_path(system.id) ,class: "btn btn-primary" %></td>
        </tr>
        <% end %>
      </tbody>
    </table>