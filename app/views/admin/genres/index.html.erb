admin/genres#index
<h1>ゲームジャンル一覧ページ</h1>
<%= link_to "記事タイプ一覧ページ", admin_types_path %>
<table class = "table">
  <thead>
    <h2>ゲームジャンル一覧・追加</h2><br>
  </thead>
  <tbody>
    <%= form_with model: @genre, url: admin_genres_path do |f| %>
      <tr><td>
      <lavel>ゲームジャンル登録</lavel>
      </td><td>
      <%= f.text_field :name %>
      </td>
      <td>
      <%= f.submit "新規登録" %>
      </td></tr>
      </tbody>
    </table>
    <% end %>
    <br>
    
    <table class = "table table-hover">
      <thead>
        <tr>
          <th>ジャンル名</th>
          <th></th>
          <th colspan="1"></th>
        </tr>
      </thead>
      <tbody>
        <% @genres.each do |genre| %>
        <tr>
          <td><%= genre.name %></td>
          <td><%= link_to "編集", edit_admin_genre_path(genre.id) ,class: "btn btn-primary" %></td>
        </tr>
        <% end %>
      </tbody>
    </table>