<div class="row">
  <div class="col-md-8 offset-md-2">
<h1>ゲームジャンル編集ページ</h1>

<h2>ジャンル編集</h2><br>
      <%= form_with model: @genre, url: admin_genre_path(@genre.id) do |f| %>
    <table class = "table">
          <tr><td>
          <lavel>ジャンル名</lavel>
          </td><td>
          <%= f.text_field :name %>
          </td><td>
          <%= f.submit "変更を保存", class: "btn btn-primary" %>
          </td></tr>
      <% end %>
  </div>
</div>