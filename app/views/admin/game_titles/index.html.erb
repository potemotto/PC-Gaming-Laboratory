admin/game_titles#index
<h1>ゲームタイトル一覧</h1>
<%= link_to "タイトル追加", new_admin_game_title_path %>
    <table class = "table table-hover">
      <thead>
        <tr>
          <th>タイプ名</th>
          <th></th>
          <th colspan="1"></th>
        </tr>
      </thead>
      <tbody>
        <% @game_titles.each do |game_title| %>
        <tr>
          <td><%= game_title.name %></td>
          <td><%= link_to "編集", edit_admin_game_title_path(game_title.id) ,class: "btn btn-primary" %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
    
    
  