<div class="row">
  <div class="col-md-8 offset-md-2">
<h1>OS情報登録ページ</h1>
    <h2>登録OS一覧</h2><br>
    <%= form_with model: @system, url: admin_systems_path do |f| %>

      OS名称
      <%= f.text_field :name %><br>

      bit数
      <%= f.select :bit_type, ["64bit","32bit"] %><br>

      上限メモリ(GB)
      <%= f.text_field :upper_memory_limit %><br>

      DirectX
      <%= f.text_field :DirectX %><br>

      リリース年
      <%= f.text_field :year_of_release %><br>


    <%= f.submit "新規登録", class: "btn btn-primary" %>
    <% end %>
